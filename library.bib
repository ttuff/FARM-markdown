month = {sep},
number = {1599},
pages = {2257--2266},
pmid = {16928626},
title = {{Climate, energy and diversity.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1636092{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {273},
year = {2006}
}
