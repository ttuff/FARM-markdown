keywords = {Acclimatization,Animals,Biological Evolution,Cell Respiration,Climate Change,Environment,Fundulidae,Fundulidae: metabolism,Fundulidae: physiology,Mitochondria,Mitochondria: metabolism,Mitochondria: physiology,Oxygen Consumption,Phenotype,Temperature,Thermodynamics,Time Factors},
month = {nov},
number = {5},
pages = {691--702},
pmid = {21841184},
title = {{Thermal performance curves, phenotypic plasticity, and the time scales of temperature exposure.}},
