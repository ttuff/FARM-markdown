journal = {Transactions in GIS},
keywords = {Generalized space-time path,Geovisualization,Individual-based dataset,Space-time GIS,Time geography},
number = {4},
pages = {425--441},
pmid = {15301},
title = {{A space-time GIS approach to exploring large individual-based spatiotemporal datasets}},
volume = {12},
year = {2008}
