keywords = {Animal,Animal: physiology,Animals,Biomechanics,Birds,Birds: physiology,Coturnix,Coturnix: physiology,Flight,Movement,Movement: physiology,Video Recording,Wing,Wing: physiology},
number = {Pt 4},
pages = {725--39},
pmid = {10648214},
title = {{Kinematics and mechanics of ground take-off in the starling Sturnis vulgaris and the quail Coturnix coturnix.}},
