journal = {Comparative biochemistry and physiology. Part A, Molecular {\&} integrative physiology},
keywords = {Animals,Body Size,Energy Metabolism,Female,Insects,Insects: metabolism,Models, Biological,Mothers,Phenotype,Quantitative Trait, Heritable},
month = {jun},
number = {2},
pages = {349--54},
pmid = {17276116},
title = {{Maternal effects, maternal body size and offspring energetics: a study in the common woodlouse Porcellio laevis.}},
