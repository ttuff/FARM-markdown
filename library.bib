issn = {00218790},
journal = {Journal of Animal Ecology},
keywords = {Biophysical model,Energy budget,Energy use and costs,Grasshopper,Metabolic rate,Phenotype,Population dynamics,Thermal tolerance},
number = {1},
pages = {41--50},
title = {{Phenotypic clines, energy balances and ecological responses to climate change}},
volume = {83},
