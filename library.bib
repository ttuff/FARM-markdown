keywords = {Alleles,Animals,Crosses, Genetic,Drosophila melanogaster,Drosophila melanogaster: genetics,Drosophila melanogaster: physiology,Feeding Behavior,Female,Larva,Male,Motor Activity,Mutation,Phenotype,X Chromosome,Y Chromosome},
