keywords = {bergmann's rule,body size,latitude},
number = {4},
pages = {165--174},
pmid = {2640},
title = {{Geographic gradients in body size: a clarification of Bergmann's rule}},
url = {http://onlinelibrary.wiley.com/doi/10.1046/j.1472-4642.1999.00046.x/full},
volume = {5},
