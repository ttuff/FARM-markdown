isbn = {0079-6107},
issn = {00796107},
journal = {Progress in Biophysics and Molecular Biology},
keywords = {Allometry,Biological rhythms,Circadian rhythms,Heartbeats,Rate variability},
number = {3},
pages = {474--484},
pmid = {21316386},
