issn = {1365-2656},
journal = {The Journal of animal ecology},
keywords = {Animals,Ecosystem,Fertility,Fertility: physiology,Finland,Models,Nesting Behavior,Passeriformes,Passeriformes: growth {\&} development,Passeriformes: physiology,Predatory Behavior,Rain,Spatial Analysis,Temperature,Theoretical,Trees},
