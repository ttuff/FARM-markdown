journal = {Cognition},
keywords = {Cross-linguistic differences,Eye-tracking,Greek,Language and thought,Motion,Spatial language,Visual world},
number = {1},
pages = {155--184},
pmid = {18395705},
title = {{Does language guide event perception? Evidence from eye movements}},
volume = {108},
year = {2008}
}
