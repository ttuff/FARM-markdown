journal = {Ecology},
keywords = {Atalopedes campestris,Climate change,Cold tolerance,Geographic range limits,Population abundance,Range shift},
number = {1},
pages = {231--241},
title = {{Warmer winters drive butterfly range expansion by increasing survivorship}},
volume = {85},
year = {2004}
