volume = {22},
year = {2007}
}
@article{Gallagher2014,
abstract = {Assisted colonization is a form of conservation translocation which introduces species at risk from extinction to new habitats, beyond their current range, in anticipation of more suitable conditions. Identifying which species, communities and ecosystems may benefit most from assisted colonization in coming decades is a key goal for conservation. Climate change is expected to lead to the loss or movement of suitable habitat for a range of species and anticipating which can be effectively conserved through assisted colonization is critical. Here, we identify a series of scenarios that may predispose terrestrial species to the need for assisted colonization in order to reduce extinction risk resulting from anthropogenic climate change and assemble a list of traits commonly associated with at-risk species. These traits may help to provide broad-scale guidance on how to select species to target for assisted colonization as a conservation management response to climate change. We also identify six key themes associated with successful conservation translocations including recipient site selection and preparation, a clear understanding of species biology and ecology, and taking lessons from invasive species research.},
author = {Gallagher, Rachael V. and Makinson, Robert O. and Hogbin, Patricia M. and Hancock, Nola},
doi = {10.1111/aec.12163},
file = {:Users/Ty/Documents/Mendeley Desktop/Gallagher et al.{\_}2014{\_}Assisted colonization as a climate change adaptation tool.pdf:pdf},
issn = {14429985},
journal = {Austral Ecology},
keywords = {Biodiversity conservation,Climate change,Managed relocation,Species trait,Translocation},
pages = {12--20},
title = {{Assisted colonization as a climate change adaptation tool}},
year = {2014}
}
