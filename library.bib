pages = {1157--1164},
pmid = {20585381},
title = {{Plasticity versus repeatability in seabird migratory behaviour.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2885296{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {64},
