issn = {1461-0248},
journal = {Ecology letters},
keywords = {Adaptation, Physiological,Adaptation, Physiological: genetics,Adaptation, Physiological: physiology,Animals,Biological Evolution,Climate Change,Ecosystem,Hot Temperature},
month = {sep},
number = {9},
pages = {1206--19},
pmid = {23869696},
title = {{Heat freezes niche evolution.}},
