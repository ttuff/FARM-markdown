issn = {1932-6203},
journal = {PloS One},
keywords = {Air,Air Microbiology,Animal,Animals,Birds,Birds: physiology,Climate,Earth (Planet),Flight,Microwaves,Oceans and Seas,Seawater,Wind},
month = {jan},
number = {8},
