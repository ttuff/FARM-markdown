isbn = {0304-3800},
issn = {03043800},
journal = {Ecological Modelling},
keywords = {Biological invasions,Biosecurity,Pest risk analysis,Policy analysis,Spatial stochastic simulation},
number = {17},
pages = {2068--2075},
title = {{Unveiling human-assisted dispersal mechanisms in invasive alien insects: Integration of spatial stochastic simulation and phenology models}},
