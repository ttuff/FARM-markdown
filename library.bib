@article{Castaneda2005,
abstract = {Extreme temperatures restrict the performance of terrestrial arthropods and variations in low temperatures on a latitudinal scale influence physiological variables. Recovery time from chill coma is a measure of cold tolerance and it is a good index of climatic adaptation. We tested differences in recovery time of the common woodlouse (Porcellio laevis) exposed to different thermal conditions. Individuals were sampled from four different populations in Chile, spanning a latitudinal range of approximately 10 degrees . Significant differences were found in recovery time among experimental temperatures and among populations, but no interaction between these factors. The results of recovery time in P. laevis showed a positive increment with annual mean minimum temperature, indicating that there is geographical variation in recovery time. While body mass presented interpopulational variation, this variation was not associated with thermal variables or latitude. Overall, our results agree with previous studies in the sense that recovery time from chill coma decreases towards high latitudes, and it is independent of taxa, continent and hemisphere.},
author = {Casta{\~{n}}eda, Luis E and Lardies, Marco A and Bozinovic, Francisco},
doi = {10.1016/j.jinsphys.2005.08.005},
file = {:Users/Ty/Documents/Mendeley Desktop/Castan, Lardies, Bozinovic{\_}2005{\_}Interpopulational variation in recovery time from chill coma along a geographic gradient A study in the.pdf:pdf},
isbn = {0022-1910},
