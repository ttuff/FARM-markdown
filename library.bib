journal = {Proceedings. Biological sciences / The Royal Society},
number = {1781},
pages = {20133039},
pmid = {24598420},
title = {{First satellite tracks of neonate sea turtles redefine the 'lost years' oceanic niche.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24598420},
volume = {281},
year = {2014}
}
