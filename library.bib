month = {jun},
number = {1664},
pages = {1939--48},
title = {{Why tropical forest lizards are vulnerable to climate warming.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2677251{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {276},
