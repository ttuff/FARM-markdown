month = {mar},
number = {3},
pages = {249--59},
pmid = {19245587},
title = {{Global biogeography and ecology of body size in birds}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19245587 http://onlinelibrary.wiley.com/doi/10.1111/j.1461-0248.2009.01281.x/full},
