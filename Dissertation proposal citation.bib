pages = {225--239},
pmid = {15556661},
title = {{Static magnetic fields: animal studies}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15556661 http://www.pnas.org/content/105/6/1965.short},
volume = {87},
year = {2005}
}
