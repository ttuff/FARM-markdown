isbn = {1461-023X},
issn = {1461023X},
journal = {Ecology Letters},
keywords = {Biodiversity patterns,Birds,Distribution atlases,Great Britain,Macroecology,More-individuals hypothesis,Productivity,Scaling,South Africa},
month = {may},
