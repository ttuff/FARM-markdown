keywords = {births,deaths,emigration,immigration,population size,range limits},
number = {1661},
pages = {1395--1406},
pmid = {19324809},
title = {{Geographic range limits: achieving synthesis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19324809},
volume = {276},
