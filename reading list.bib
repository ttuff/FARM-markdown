pages = {1961--1964},
pmid = {19818616},
title = {{Weather and seasons together demand complex biological clocks}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19818616 http://www.sciencedirect.com/science/article/pii/S0960982209017047},
